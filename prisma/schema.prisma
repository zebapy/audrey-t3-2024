generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

enum ServingUnit {
  GRAM
  MILLILITER
  CUP
  TABLESPOON
  TEASPOON
  PIECE
}

enum LogTimingGroup {
  BREAKFAST
  LUNCH
  DINNER
  SNACK
  UNCATAGORIZED
}

model FoodLog {
  id        String         @id @default(uuid())
  foodId    String
  userId    String
  createdAt DateTime       @default(now())
  updatedAt DateTime       @updatedAt
  servings  Float
  unit      ServingUnit
  food      openfoodfacts  @relation(fields: [foodId], references: [id])
  group     LogTimingGroup @default(UNCATAGORIZED)

  @@map("food_logs")
}

